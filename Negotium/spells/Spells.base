views:
  - type: cards
    name: Table
    filters:
      and:
        - file.inFolder("Negotium/spells/list_spells")
        - '!file.fullname.endsWith(".png")'
    order:
      - file.name
    sort:
      - property: file.name
        direction: ASC
    image: note.image
    cardSize: 190
    imageAspectRatio: 1
    imageFit: contain
